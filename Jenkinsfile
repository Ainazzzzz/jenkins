pipeline {
    agent any

    stages {
        stage('Pull') {
            steps {
                echo 'Pulling the source code'
              
            }
        }

        stage('Run Tests') {
            steps {
                echo 'Running tests'
               
            }
        }

        stage('Build') {
            steps {
                echo 'Building the project'
              
            }
        }
    }

}
