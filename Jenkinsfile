pipeline {
    agent any

    stages {
        stage('Pull') {
            steps {
                echo 'Pulling the source code'
              
            }
        }

        stage('Run Tests') {
            steps {
                echo 'Running tests'
               
                script {
                    
                }
            }
        }

        stage('Build') {
            steps {
                echo 'Building the project'
              
                script {
                  
                }
            }
        }
    }

    post {
        success {
            echo 'Pipeline succeeded!'
          
        }
        failure {
            echo 'Pipeline failed!'
           
        }
    }
}
